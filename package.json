		"@types/parse-color": "1.0.0",
		"@types/roll": "1.2.0",
		"@types/signale": "1.4.1",
		"@types/ws": "7.2.4",
		"eslint-plugin-jest": "23.9.0",
		"jest": "26.0.1",
		"nyc": "15.0.1",
		"prettier": "2.0.4",
		"rimraf": "3.0.2",
		"semantic-release": "17.0.7",
		"source-map-support": "0.5.19",
		"ts-jest": "25.5.0",
		"ts-node": "8.10.1",
		"typescript": "3.8.3",
		"xo": "0.30.0"
	},
	"engines": {
		"node": "12"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/src/index.js",
	"name": "dice",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dice-discord/bot.git"
	},
	"scripts": {
		"build": "tsc",
		"deploy": "semantic-release",
		"lint": "xo",
		"postinstall": "prisma generate",
		"prebuild": "rimraf -rf tsc_output",
		"start": "node tsc_output/src/index.js",
		"style": "prettier --ignore-path .gitignore --check .",
		"test": "jest"
	},
	"version": "4.6.1",
	"xo": {
		"extends": [
			"plugin:jest/recommended"
		],
		"plugins": [
			"jest"
		],
		"prettier": true,
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": [
				"off"
			],
			"camelcase": [
				"error",
				{
					"allow": [
						"id_guildId"
					]
				}
			],
			"unicorn/filename-case": [
				"error",
				{
					"cases": {
						"camelCase": true,
						"kebabCase": true,
						"pascalCase": true
					}
				}
			]
		}
	}
}
