		"convert": "1.1.1",
		"cron": "1.8.2",
		"date-fns": "2.16.1",
		"delay": "4.4.0",
		"discord-akairo": "8.1.0",
		"discord-md-tags": "1.0.0",
		"discord.js": "12.2.0",
		"dotenv": "8.2.0",
		"erlpack": "discordapp/erlpack",
		"escape-string-regexp": "4.0.0",
		"got": "11.8.0",
		"influx": "5.6.3",
		"kurasuta": "2.1.0",
		"micri": "4.2.1",
		"parse-color": "1.0.0",
		"pretty-ms": "7.0.1",
		"raw-body": "2.4.1",
		"roll": "1.3.1",
		"semantic-release-docker": "2.2.0",
		"signale": "1.4.0",
		"snyk": "1.432.1",
		"sqreen": "1.56.1",
		"utf-8-validate": "5.0.3",
		"zlib-sync": "0.1.7"
	},
	"devDependencies": {
		"@eclass/semantic-release-sentry-releases": "2.7.0",
		"@prisma/cli": "2.12.1",
		"@semantic-release/exec": "5.0.0",
		"@tsconfig/node14": "1.0.0",
		"@types/cron": "1.7.2",
		"@types/jest": "26.0.15",
		"@types/node": "12.19.8",
		"@types/parse-color": "1.0.0",
		"@types/roll": "1.2.0",
		"@types/signale": "1.4.1",
		"@types/ws": "7.4.0",
		"eslint-plugin-jest": "24.1.3",
		"eslint-plugin-prettier": "3.1.4",
		"jest": "26.6.3",
		"nyc": "15.1.0",
		"prettier": "2.2.1",
		"rimraf": "3.0.2",
		"semantic-release": "17.3.0",
		"source-map-support": "0.5.19",
		"ts-jest": "26.4.4",
		"ts-node": "9.0.0",
		"type-fest": "0.20.2",
		"typescript": "4.1.2",
		"xo": "0.35.0"
	},
	"engines": {
		"node": "14 || 15"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/src/index.js",
	"name": "dice",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dice-discord/bot.git"
	},
	"scripts": {
		"build": "tsc",
		"deploy": "semantic-release",
		"docs": "node tsc_output/src/docs",
		"lint": "xo",
		"postinstall": "prisma generate",
		"prebuild": "rimraf tsc_output/**.js",
		"predocs": "rimraf tsc_output/command_docs",
		"prepublish": "npm run snyk-protect",
		"snyk-protect": "snyk protect",
		"start": "node tsc_output/src/index.js",
		"style": "prettier --ignore-path .gitignore --check .",
		"test": "jest"
	},
	"snyk": true,
	"version": "4.15.9",
	"xo": {
		"extends": [
			"plugin:jest/recommended"
		],
		"plugins": [
			"jest"
		],
		"prettier": true,
		"rules": {
			"camelcase": [
				"error",
				{
					"allow": [
						"id_guildId"
					]
				}
			],
			"node/prefer-global/url": "off",
			"node/prefer-global/url-search-params": "off",
			"unicorn/filename-case": [
				"error",
				{
					"cases": {
						"camelCase": true,
						"kebabCase": true,
						"pascalCase": true
					}
				}
			]
		}
	}
}
